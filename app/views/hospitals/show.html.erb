<p>
  <b>Name:</b>
  <%= @hospital.name %>
</p>

<p>
  <b>Location city:</b>
  <%= @hospital.location_city %>
</p>

<p>
  <b>Location area:</b>
  <%= @hospital.location_area %>
</p>

<p>
  <b>Location logitude:</b>
  <%= @hospital.location_logitude %>
</p>

<p>
  <b>Location latitude:</b>
  <%= @hospital.location_latitude %>
</p>

<p>
  <b>Tel number:</b>
  <%= @hospital.tel_number %>
</p>

<p>
  <b>Department:</b>
  <%= @hospital.department %>
</p>

<p>
  <b>Detail:</b>
  <%= @hospital.detail %>
</p>

<% @hospital.photos.each do |photo| %>
  <p>
    <%= image_tag photo.medium_url %>
  </p>
<% end %>


<%= link_to 'Edit', edit_hospital_path(@hospital) %> |
<%= link_to 'Back', hospitals_path %>
